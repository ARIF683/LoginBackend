{
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "filesystem": {
        "publicFiles": [
            {
                "source": "/public/(.*)",
                "destination": "/$1"
            }
        ]
    }
    
}