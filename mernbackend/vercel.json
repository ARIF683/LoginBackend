{
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "filesystem": {
        "publicFiles": [
            {
                "source": "/public/(.*)",
                "destination": "/$1"
            }
        ]
    },
    "$schema": "./node_modules/@vercel/build-utils/schema.json"
}